<?xml version="1.0" encoding="UTF-8"?>
<numericalExperiment documentVersion="1" control="false" xmlns="http://www.metaforclimate.eu/cim/1.4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.metaforclimate.eu/cim/1.4 Z:\CIM\XSDs\dev0p3\activity.xsd" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco">
	
    <rationale>C4MIP style experiment to further understand "slower" carbon cycle responses (ie. feedbacks). </rationale>
    <description>Impose conditions identical to 3.1::Control but radiation code sees CO2 concentration increase. </description>

	<!-- Initial Conditions -->
    <numericalRequirement xsi:type="InitialCondition">
        <id xsi:type="Identifier">ic.001</id>
        <name xsi:type="Identifier">5.5-1.ic</name>
        <description>A description of initial condition for the control run </description>
    </numericalRequirement>
    
	<!-- Boundary Conditions -->
	<numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.001</id>
        <name xsi:type="Identifier">5.5-1.bc.wmg_conc</name>
        <description>Prescribed atmospheric concentrations of pre-industrial well mixed gases:
            excluding CO2</description>
    </numericalRequirement>
        
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.003</id>
        <name xsi:type="Identifier">5.5-1.bc.sls_conc</name>
        <description>Prescribed atmospheric concentration of pre-industrial short lived (reactive)
            gas species</description>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <name xsi:type="Identifier">5.5-1.bc.nat_aer</name>
        <description>Prescribed concentrations or emissions of pre-industrial natural aerosols</description>     
        <requirementOption>
            <id xsi:type="Identifier">bc.004</id>
            <name xsi:type="Identifier">5.5-1.bc.nat_aer_conc</name>
            <description>Concentrations</description>            
        </requirementOption>
        <requirementOption>
            <id xsi:type="Identifier">bc.007</id>
            <name xsi:type="Identifier">5.5-1.bc.nat_aer_em</name>
            <description>Emissions</description>            
        </requirementOption>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <name xsi:type="Identifier">5.5-1.bc.nat_aer_pre</name>
        <description>Prescribed concentrations or emissions of pre-industrial natural aerosol  precursors</description>
        <requirementOption>
            <id xsi:type="Identifier">bc.005</id>
            <name xsi:type="Identifier">5.5-1.bc.nat_aer_pre_conc</name>
            <description>Concentrations</description>            
        </requirementOption>
        <requirementOption>
            <id xsi:type="Identifier">bc.008</id>
            <name xsi:type="Identifier">5.5-1.bc.nat_aer_pre_em</name>
            <description>Emissions</description>            
        </requirementOption>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <name xsi:type="Identifier">5.5-1.bc.sl_aer</name>
        <description>Prescribed concentrations or emission of pre-industrial short lived (reactive) aerosol  species</description>
        <requirementOption>
            <id xsi:type="Identifier">bc.006</id>
            <name xsi:type="Identifier">5.5-1.bc.sl_aer_conc</name>
            <description>Concentrations</description>            
        </requirementOption>
        <requirementOption>
            <id xsi:type="Identifier">bc.009</id>
            <name xsi:type="Identifier">5.5-1.bc.sl_aer_em</name>
            <description>Emissions</description>
        </requirementOption>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.010</id>
        <name xsi:type="Identifier">5.5-1.bc.LU</name>
        <description>Unperturbed Pre-Industrial Land Use</description>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">	
		<id xsi:type="Identifier">bc.098a</id>
		<name xsi:type="Identifier">5.5-1.bc.rad </name>
		<description>Forced with prescribed CO2 concentrations following spin off 
		    from the control (expt. 3.1) at the same point as expt. 6.1 and impose 
		    conditions identical to the control except that the carbon cycle 
		    &quot;sees&quot; atmospheric CO2 concentration increase at a rate of 
		    1%/yr (as in expt. 6.1). The carbon cycle responds to the climate 
		    change alone.
		</description>
	</numericalRequirement>
		
		
	<shortName>
		5.5-1 esmFdbk1
	</shortName>
    <longName> 
		5.5-1 ESM feedback 1
	</longName>
	
	<calendar>
	    <realCalendar/>
	</calendar>
    <numericalRequirement xsi:type="SpatioTemporalConstraint"><name xsi:type="Identifier">5.5-1.stc.1850_140yr</name><id xsi:type="Identifier">stc.033</id><description>Begin in 1850 and run for 140 years </description><requiredDuration xsi:type="ClosedDateRange">
         <startDate>1850-01-01T00:00:00Z</startDate>
        <endDate>1990-12-30T00:00:00Z</endDate>
        <length units="years">140</length>
    </requiredDuration></numericalRequirement>
	
