<?xml version="1.0" encoding="UTF-8"?>
<numericalExperiment documentVersion="1" control="false" xmlns="http://www.metaforclimate.eu/cim/1.4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.metaforclimate.eu/cim/1.4 Z:\CIM\XSDs\dev0p3\activity.xsd" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco">
    
    <rationale>Provide estimate of future anthropogenic climate change.</rationale>
	<description>Extension of RCP2.6 through to the year 2300.</description>
	
	<!-- Initial Conditions -->
	<numericalRequirement xsi:type="InitialCondition">
        <id xsi:type="Identifier">ic.004c</id>
        <name xsi:type="Identifier">4.3L.ic</name>
        <description> Initial conditions are from the end of experiment 4.3 RCP2.6.</description>
    </numericalRequirement>
    
    <!-- Boundary/Forcing Conditions -->
    <numericalRequirement xsi:type="BoundaryCondition">
        <name xsi:type="Identifier">4.3L.bc.wmg</name>
        <description>Imposed changing concentrations or emissions of extended RCP2.6 well mixed gases, including CO2</description>
        <requirementOption>
            <id xsi:type="Identifier">bc.076</id>
            <name xsi:type="Identifier">4.3L.bc.wmg_conc</name>
            <description>Concentrations</description>            
        </requirementOption>
        <requirementOption>
            <id xsi:type="Identifier">bc.080</id>
            <name xsi:type="Identifier">4.3L.bc.wmg_em</name>
            <description>Emissions</description>            
        </requirementOption>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <name xsi:type="Identifier">4.3L.bc.sls</name>
        <description>Imposed changing concentrations or emissions of extended RCP2.6 short lived gas species </description>
        <requirementOption>
            <id xsi:type="Identifier">bc.077</id>
            <name xsi:type="Identifier">4.3L.bc.sls_conc</name>
            <description>Concentrations</description>            
        </requirementOption>
        <requirementOption>
            <id xsi:type="Identifier">bc.081</id>
            <name xsi:type="Identifier">4.3L.bc.sls_em</name>
            <description>Emissions</description>            
        </requirementOption>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <name xsi:type="Identifier">4.3L.bc.aer</name>
        <description>Imposed changing concentrations or emissions of extended RCP2.6 aerosols</description>
        <requirementOption>
            <id xsi:type="Identifier">bc.078</id>
            <name xsi:type="Identifier">4.3L.bc.aer_conc</name>
            <description>Concentrations</description>            
        </requirementOption>
        <requirementOption>
            <id xsi:type="Identifier">bc.082</id>
            <name xsi:type="Identifier">4.3L.bc.aer_em</name>
            <description>Emissions</description>            
        </requirementOption>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <name xsi:type="Identifier">4.3L.bc.aer_pre</name>
        <description>Imposed changing concentrations or emissions of extended RCP2.6 aerosol precursors </description>
        <requirementOption>
            <id xsi:type="Identifier">bc.079</id>
            <name xsi:type="Identifier">4.3L.bc.aer_pre_conc</name>
            <description>Concentrations</description>            
        </requirementOption>
        <requirementOption>
            <id xsi:type="Identifier">bc.083</id>
            <name xsi:type="Identifier">4.3L.bc.aer_pre_em</name>
            <description>Emissions</description>            
        </requirementOption>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.084</id>
        <name >4.3L.bc.LU</name>
        <description>Imposed changing RCP2.6 land use</description>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.034</id>
        <name xsi:type="Identifier">4.3L.bc.volc_aer_conc</name>
        <description>Imposed constant background volcanic aerosol</description>
    </numericalRequirement>
    
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.095</id>
        <name xsi:type="Identifier">4.3L.bc.ext2300</name>
        <description>RCP2.6 conditions extended through year 2300</description>
    </numericalRequirement>
    
    <shortName>
		4.3-L rcp26
	</shortName>
    <longName> 
		4.3-L RCP2.6 - Extended
	</longName>
    
    <calendar>
        <realCalendar/>
    </calendar>
    <numericalRequirement xsi:type="SpatioTemporalConstraint"><name xsi:type="Identifier">4.3L.stc.2100_200yr</name><id xsi:type="Identifier">stc.031</id><description>Begin in 2100 and run for 200 years </description><requiredDuration xsi:type="ClosedDateRange">
		<startDate>2100-01-01T00:00:00Z</startDate>
        <endDate>2300-01-01T00:00:00Z</endDate>
	    <length units="years">200</length>
	</requiredDuration></numericalRequirement>
	
